{
  "name": "@classdo/classdo",
  "version": "0.2.1",
  "description": "ClassDo API client for browsers and NodeJS.",
  "keywords": [
    "ClassDo",
    "collaboration",
    "video chat",
    "virtual room"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "typedoc": "typedoc --mode file --out docs/ ./src",
    "codegen": "graphql-codegen --config codegen.yml",
    "test": "jest",
    "clean": "rm -rf dist",
    "build": "yarn clean && tsc -p .",
    "release": "standard-version",
    "prerelease": "yarn test && yarn build"
  },
  "bugs": {
    "url": "https://github.com/ClassDo/classdo-js/issues"
  },
  "homepage": "https://github.com/ClassDo/classdo-js",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ClassDo/classdo-js.git"
  },
  "author": "ClassDo",
  "license": "MIT",
  "dependencies": {
    "apollo": "^2.21.2",
    "apollo-cache-inmemory": "^1.6.5",
    "apollo-client": "^2.6.8",
    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "node-fetch": "^2.6.0",
    "typed-graphqlify": "^2.2.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@graphql-codegen/cli": "^1.11.2",
    "@graphql-codegen/typescript": "^1.13.1",
    "@types/jest": "^26.0.0",
    "@types/node-fetch": "^2.5.4",
    "graphql": "^15.5.1",
    "husky": "^4.2.3",
    "jest": "^25.1.0",
    "standard-version": "^9.0.0",
    "ts-jest": "^25.2.0",
    "typedoc": "^0.19.1",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
