    "apollo-link": "^1.2.13",
    "apollo-link-context": "^1.0.19",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "loglevel": "^1.6.6",
    "node-fetch": "^2.6.0",
    "typed-graphqlify": "^2.2.3",
    "yarn": "^1.21.1"
